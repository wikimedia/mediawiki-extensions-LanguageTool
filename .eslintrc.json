{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"ve": false
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-jquery/no-global-selector": "warn",
		"no-useless-call": "warn",
		"valid-jsdoc": "off",
		"new-cap": "off",
		"prefer-const": "warn",
		"no-jquery/no-done-fail": "warn"
	}
}
